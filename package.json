{
  "name": "StreamHub-MultiView",
  "description": "a pluggable View for use with StreamHub-Backbone. Uses the jQuery-Isotope plugin to lay out Content for a tiled display.",
  "author": {
    "name": "Derek Chinn"
  },
  "version": "0.0.0",
  "devDependencies": {
    "bower": "*",
    "http-server": "*",
    "jsdoc": "git://github.com/jsdoc3/jsdoc.git"
  },
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower install",
    "start": "./node_modules/http-server/bin/http-server .",
    "jsdoc": "rm -rf docs/ && node_modules/jsdoc/jsdoc -c tools/jsdoc.conf.json"
  }
}
